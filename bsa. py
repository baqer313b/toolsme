def uaku():
        try:
                ua=open('bbnew.txt','r').read().splitlines()
                for ub in ua:
                        ugen.append(ub)
        except:
                a=requests.get('https://raw.githubusercontent.com/PSYCHO-PICCHI/ua/main/bbnew.txt').text
                ua=open('.bbnew.txt','w')
                aa=re.findall('line">(.*?)<',str(a))
                for un in aa:
                        ua.write(un+'\n')
                ua=open('.bbnew.txt','r').read().splitlines()

