def passwrd():

    with tred(max_workers=30) as pool:
        for yuzong in id2:
            idf,nmf = yuzong.split('|')[0],yuzong.split('|')[1].lower()
            frs = nmf.split(' ')[0]
            pwv = []
            if len(nmf)<6:
                if len(frs)<3:
                    pass
                else:
                    pwv.append(nmf)
                    pwv.append('frs+frs')
                    pwv.append(frs+'123')
                    pwv.append(frs+'1234')
                    pwv.append('firstlast')
